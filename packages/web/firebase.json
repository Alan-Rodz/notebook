{
  "hosting": {
    "public": "src/public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ]
  },
  "functions": {
    "source": "."
  },
  "emulators": {
    "hosting": {
      "port": 3000
    },
    "functions": {
      "port": 5001
    },
    "ui": {
      "enabled": true
    }
  }
}